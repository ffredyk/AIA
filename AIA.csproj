<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows10.0.22000.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <AssemblyVersion>1.0.7.253</AssemblyVersion>
    <FileVersion>1.0.7.253</FileVersion>
	<UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>Icons\AIA.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Icons\AIA.ico" />
    <None Remove="Icons\AIA_s.ico" />
    <None Remove="Icons\AIA_sg.ico" />
    <None Remove="Icons\AIA_sr.ico" />
    <None Remove="Icons\AIA_t.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Icons\AIA.ico" />
    <EmbeddedResource Include="Icons\AIA_s.ico" />
    <EmbeddedResource Include="Icons\AIA_sg.ico" />
    <EmbeddedResource Include="Icons\AIA_sr.ico" />
    <Resource Include="Icons\AIA_t.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="9.0.0" />
    <PackageReference Include="WPF-UI" Version="4.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="AIWrap">
      <HintPath>..\AIWrap\bin\Debug\net10.0\AIWrap.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Converters\" />
  </ItemGroup>

</Project>
